{
	"extends": "../tsconfig",
	"compilerOptions": {
		"outDir": "dist-node",
                "skipLibCheck": true,
	},
	"include": ["src/**/*"],
       "typedocOptions": {
                "entryPoints": ["src/index.ts"],
                "exclude": ["**/node_modules/**"],
                "externalPattern": ["**/node_modules/**"],
                "excludeExternals": true,
                "out": "docs"
        },
	"exclude": ["src/modules/dpos/*", "src/modules/sequence/*", "src/modules/keys/*"]
}
